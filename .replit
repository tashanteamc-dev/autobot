modules = ["nodejs-20"]

[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Telegram Bot"

# Workflow utama
[[workflows.workflow]]
name = "Telegram Bot"
author = "agent"

  [[workflows.workflow.tasks]]
  task = "shell.exec"
  args = "npm start"
  waitForPort = 3000

[workflows.workflow.metadata]
outputType = "console"

# Port mapping
[[ports]]
localPort = 3000
externalPort = 80
